{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "ReputationRank.json",
  "title": "ReputationRank",
  "description": "ReputationRank asset type",
  "type": "object",
  "allOf": [
    {
      "$ref": "base.schema.json"
    }
  ],
  "properties": {
    "MinValue": {
      "type": "integer",
      "default": 0
    },
    "MaxValue": {
      "type": "integer",
      "default": 0
    },
    "Attitude": {
      "type": [
        "string",
        "null"
      ],
      "title": "Attitude",
      "enum": [
        "Ignore",
        "Hostile",
        "Neutral",
        "Friendly",
        "Revered"
      ]
    }
  }
}