      "type": "string",
      "title": "ParticleRotationInfluence",
      "enumDescriptions": [
        "",
        "",
        "",
        "",
        ""
      ],
      "markdownEnumDescriptions": [
        "",
        "",
        "",
        "",
        ""
      ],
      "hytale": {
        "type": "Enum",
        "inheritsProperty": true,
        "uiSectionStart": "Motion"
      },
      "enum": [
        "None",
        "Billboard",
        "BillboardY",
        "BillboardVelocity",
        "Velocity"
      ],
      "default": "None"
