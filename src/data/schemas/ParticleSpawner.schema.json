    "ParticleCollision": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/ParticleCollision",
          "hytale": {
            "inheritsProperty": true,
            "uiSectionStart": "Collision",
            "uiCollapsedByDefault": false
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "IsLowRes": {
      "type": "boolean",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true,
        "uiSectionStart": "Optimization"
      },
      "default": false
    },
    "Particle": {
      "$ref": "common.json#/definitions/Particle",
      "hytale": {
        "inheritsProperty": true,
        "uiSectionStart": "Particle",
        "uiCollapsedByDefault": false
      }
    }
  }
}