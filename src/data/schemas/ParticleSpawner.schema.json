{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "ParticleSpawner.json",
  "title": "ParticleSpawner",
  "description": "ParticleSpawner asset type",
  "type": "object",
  "allOf": [
    {
      "$ref": "base.schema.json"
    }
  ],
  "properties": {
    "Shape": {
      "type": "string",
      "title": "EmitShape",
      "enumDescriptions": [
        "",
        ""
      ],
      "markdownEnumDescriptions": [
        "",
        ""
      ],
      "hytale": {
        "type": "Enum",
        "inheritsProperty": true
      },
      "enum": [
        "Sphere",
        "Cube"
      ],
      "default": "Sphere"
    },
    "EmitOffset": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/RangeVector3f",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "UseEmitDirection": {
      "type": "boolean",
      "description": "Use spawn position to determine direction. Overrides pitch/yaw in InitialVelocity.",
      "markdownDescription": "Use spawn position to determine direction. Overrides pitch/yaw in InitialVelocity.",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true
      },
      "default": false
    },
    "TotalParticles": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/Range",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "LifeSpan": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number",
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "MaxConcurrentParticles": {
      "type": "integer",
      "hytale": {
        "type": "integer",
        "inheritsProperty": true
      },
      "default": 0
    },
    "ParticleLifeSpan": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/Rangef",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "SpawnRate": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/Rangef",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "SpawnBurst": {
      "type": "boolean",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true
      },
      "default": false
