    "TrailSpawnerPositionMultiplier": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "minimum": 0,
              "maximum": 1,
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number",
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "TrailSpawnerRotationMultiplier": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "minimum": 0,
              "maximum": 1,
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number",
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "VelocityStretchMultiplier": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number"
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "Attractors": {
      "type": [
        "array",
        "null"
      ],
      "hytale": {
        "type": "array",
        "inheritsProperty": true,
        "uiSectionStart": "Attractors",
        "uiCollapsedByDefault": false
      },
      "items": {
        "$ref": "common.json#/definitions/ParticleAttractor"
      }
    },
    "RenderMode": {
      "type": "string",
      "title": "FXRenderMode",
      "enumDescriptions": [
        "",
        "",
        "",
        ""
      ],
      "markdownEnumDescriptions": [
        "",
        "",
        "",
        ""
      ],
      "hytale": {
        "type": "Enum",
        "inheritsProperty": true,
        "uiSectionStart": "Material"
      },
      "enum": [
        "BlendLinear",
        "BlendAdd",
        "Erosion",
        "Distortion"
      ],
      "default": "BlendLinear"
    },
    "LightInfluence": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number",
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "IntersectionHighlight": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/IntersectionHighlight",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "LinearFiltering": {
      "type": "boolean",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true
      },
      "default": false
    },
    "UVMotion": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/UVMotion",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "CameraOffset": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "minimum": -10,
              "maximum": 10,
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number"
          }
        },
        {
          "type": "null"
        }
      ]
