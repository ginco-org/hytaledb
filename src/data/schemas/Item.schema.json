      "markdownDescription": "The model used for rendering this item. If this is a block, BlockType.Model should be used instead.",
      "hytale": {
        "type": "string",
        "inheritsProperty": true,
        "uiPropertyTitle": "Item Model",
        "uiSectionStart": "Rendering",
        "uiRebuildCaches": [
          "Models"
        ],
        "uiRebuildCachesForChildProperties": false
      },
      "hytaleCommonAsset": {
        "requiredRoots": [
          "Blocks/",
          "Items/",
          "Resources/",
          "NPC/",
          "VFX/",
          "Consumable/"
        ],
