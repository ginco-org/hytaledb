{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "Item.json",
  "title": "Item",
  "description": "Item asset type",
  "type": "object",
  "allOf": [
    {
      "$ref": "base.schema.json"
    }
  ],
  "properties": {
    "Icon": {
      "type": [
        "string",
        "null"
      ],
      "hytale": {
        "type": "string",
        "inheritsProperty": true,
        "uiEditorComponent": {
          "component": "Icon",
          "defaultPathTemplate": "Icons/ItemsGenerated/{assetId}.png",
          "width": 64,
          "height": 64
        },
        "uiRebuildCaches": [
          "ItemIcons"
        ],
        "uiRebuildCachesForChildProperties": true
      },
      "hytaleCommonAsset": {
        "requiredRoots": [
          "Icons/ItemsGenerated/",
          "Icons/Items/"
        ],
        "requiredExtension": "png",
        "isUIAsset": false
      }
    },
    "Categories": {
      "type": [
        "array",
        "null"
      ],
      "description": "A list of categories this item will be shown in on the creative library menu.",
      "markdownDescription": "A list of categories this item will be shown in on the creative library menu.",
      "hytale": {
        "type": "array",
        "inheritsProperty": true
      },
      "items": {
        "type": "string",
        "hytaleAssetRef": "ItemCategory",
        "hytale": {
          "type": "string",
          "uiEditorComponent": {
            "component": "Dropdown",
            "dataSet": "ItemCategories"
          }
        }
      }
    },
    "IconProperties": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/AssetIconProperties",
          "hytale": {
            "inheritsProperty": true,
            "uiDisplayMode": "Hidden"
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "TranslationProperties": {
      "description": "The translation properties for this item asset.",
      "markdownDescription": "The translation properties for this item asset.",
      "anyOf": [
        {
          "$ref": "common.json#/definitions/ItemTranslationProperties",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "ItemLevel": {
      "type": "integer",
      "hytale": {
        "type": "integer",
        "inheritsProperty": true
      },
      "default": 0
    },
    "Reticle": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "string",
              "title": "Reference to ItemReticleConfig",
              "hytale": {
                "type": "string"
              }
            },
            {
              "$ref": "ItemReticleConfig.json#"
            }
          ],
          "hytaleAssetRef": "ItemReticleConfig",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "MaxStack": {
      "type": "integer",
      "description": "The maximum amount this item can be stacked in the inventory",
      "markdownDescription": "The maximum amount this item can be stacked in the inventory",
      "hytale": {
        "type": "integer",
        "inheritsProperty": true,
        "uiPropertyTitle": "Max Stack Size"
      },
      "exclusiveMinimum": 0,
      "default": -1
    },
    "Quality": {
      "type": [
        "string",
        "null"
      ],
      "hytaleAssetRef": "ItemQuality",
      "hytale": {
        "type": "string"
      }
    },
    "ItemEntity": {
      "$ref": "common.json#/definitions/ItemEntityConfig",
