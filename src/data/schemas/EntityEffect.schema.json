{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "EntityEffect.json",
  "title": "EntityEffect",
  "description": "EntityEffect asset type",
  "type": "object",
  "allOf": [
    {
      "$ref": "base.schema.json"
    }
  ],
  "properties": {
    "Name": {
      "type": [
        "string",
        "null"
      ],
      "description": "The name of this entity effect that will be displayed in the UI. This must be a localization key.",
      "markdownDescription": "The name of this entity effect that will be displayed in the UI. This must be a localization key.",
      "hytale": {
        "type": "string",
        "inheritsProperty": true
      }
    },
    "ApplicationEffects": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/ApplicationEffects",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "WorldRemovalSoundEventId": {
      "type": [
        "string",
        "null"
      ],
      "hytaleAssetRef": "SoundEvent",
      "hytale": {
        "type": "string",
        "inheritsProperty": true
      }
    },
    "LocalRemovalSoundEventId": {
      "type": [
        "string",
        "null"
      ],
      "hytaleAssetRef": "SoundEvent",
      "hytale": {
        "type": "string",
        "inheritsProperty": true
      }
    },
    "DamageCalculator": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/DamageCalculator",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "DamageCalculatorCooldown": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "minimum": 0,
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number",
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "DamageEffects": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/DamageEffects",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "StatModifierEffects": {
      "description": "Effects to play when stat modifiers are applied and updated.",
      "markdownDescription": "Effects to play when stat modifiers are applied and updated.",
      "anyOf": [
        {
          "$ref": "common.json#/definitions/DamageEffects",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "ModelOverride": {
      "anyOf": [
        {
          "$ref": "common.json#/definitions/ModelOverride",
          "hytale": {
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "ModelChange": {
      "type": [
        "string",
        "null"
      ],
      "description": "A model to change the affected entity's appearance to.",
      "markdownDescription": "A model to change the affected entity's appearance to.",
      "hytaleAssetRef": "ModelAsset",
      "hytale": {
        "type": "string",
        "inheritsProperty": true
      }
    },
    "RawStatModifiers": {
      "type": [
        "object",
        "null"
      ],
      "title": "Map",
      "hytale": {
        "type": "object"
      },
      "additionalProperties": {
        "type": "array",
        "hytale": {
          "type": "array"
        },
        "items": {
          "$ref": "common.json#/definitions/StaticModifier"
        }
      },
      "propertyNames": {
        "hytaleAssetRef": "EntityStatType",
        "hytale": {
          "type": "string"
        }
      }
    },
    "StatModifiers": {
      "type": [
        "object",
        "null"
      ],
      "title": "Map of string to float",
      "description": "Modifiers to apply to EntityStats.",
      "markdownDescription": "Modifiers to apply to EntityStats.",
      "hytale": {
        "type": "object",
        "inheritsProperty": true
      },
      "additionalProperties": {
        "type": "number",
        "hytale": {
          "type": "number"
        }
      },
      "propertyNames": {
        "hytaleAssetRef": "EntityStatType",
        "hytale": {
          "type": "string"
        }
      }
    },
    "ValueType": {
      "type": "string",
      "title": "ValueType",
      "description": "Enum to specify if the StatModifiers must be considered as absolute values or percent. Default value is Absolute. When using ValueType.Absolute, '100' matches the max value.",
      "markdownDescription": "Enum to specify if the StatModifiers must be considered as absolute values or percent. Default value is Absolute. When using ValueType.Absolute, '100' matches the max value.",
      "enumDescriptions": [
        "",
        ""
      ],
      "markdownEnumDescriptions": [
        "",
        ""
      ],
      "hytale": {
        "type": "Enum",
        "inheritsProperty": true
      },
      "enum": [
        "Percent",
        "Absolute"
      ],
      "default": "Absolute"
    },
    "Duration": {
      "description": "Value used by default unless specified otherwise in the method's call.",
      "markdownDescription": "Value used by default unless specified otherwise in the method's call.",
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number",
            "inheritsProperty": true
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "OverlapBehavior": {
      "type": [
        "string",
        "null"
      ],
      "title": "OverlapBehavior",
      "description": "Value used by default unless specified otherwise in the method's call.",
      "markdownDescription": "Value used by default unless specified otherwise in the method's call.",
      "enumDescriptions": [
        "",
        "",
        ""
      ],
      "markdownEnumDescriptions": [
        "",
        "",
        ""
      ],
      "hytale": {
        "type": "Enum",
        "inheritsProperty": true
      },
      "enum": [
        "Extend",
        "Overwrite",
        "Ignore"
      ],
      "default": "Ignore"
    },
    "Infinite": {
      "type": "boolean",
      "description": "Value used by default unless specified otherwise in the method's call.",
      "markdownDescription": "Value used by default unless specified otherwise in the method's call.",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true
      },
      "default": false
    },
    "Debuff": {
      "type": "boolean",
      "description": "Value used by default unless specified otherwise in the method's call.",
      "markdownDescription": "Value used by default unless specified otherwise in the method's call.",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true
      },
      "default": false
    },
    "Locale": {
      "type": [
        "string",
        "null"
      ],
      "description": "An optional translation key, used to display the damage cause upon death.",
      "markdownDescription": "An optional translation key, used to display the damage cause upon death.",
      "hytale": {
        "type": "string",
        "inheritsProperty": true
      }
    },
    "StatusEffectIcon": {
      "type": [
        "string",
        "null"
      ],
      "description": "Value used by default unless specified otherwise in the method's call.",
      "markdownDescription": "Value used by default unless specified otherwise in the method's call.",
      "hytale": {
        "type": "string",
        "inheritsProperty": true
      }
    },
    "RemovalBehavior": {
      "type": [
        "string",
        "null"
      ],
      "title": "RemovalBehavior",
      "description": "Value used by default unless specified otherwise in the method's call.",
      "markdownDescription": "Value used by default unless specified otherwise in the method's call.",
      "enumDescriptions": [
        "",
        "",
        ""
      ],
      "markdownEnumDescriptions": [
        "",
        "",
        ""
      ],
      "hytale": {
        "type": "Enum",
        "inheritsProperty": true
      },
      "enum": [
        "Complete",
        "Infinite",
        "Duration"
      ],
      "default": "Complete"
    },
    "Invulnerable": {
      "type": "boolean",
      "description": "Determines whether this effect applies the invulnerable component to the entity whilst active.",
      "markdownDescription": "Determines whether this effect applies the invulnerable component to the entity whilst active.",
      "hytale": {
        "type": "boolean",
        "inheritsProperty": true
      },
      "default": false
    },
    "DamageResistance": {
      "type": [
        "object",
        "null"
      ],
      "title": "Map",
      "hytale": {
        "type": "object",
        "inheritsProperty": true
      },
      "additionalProperties": {
        "type": "array",
        "hytale": {
          "type": "array"
        },
        "items": {
          "$ref": "common.json#/definitions/StaticModifier"
        }
      },
      "propertyNames": {
        "hytaleAssetRef": "DamageCause",
        "hytale": {
          "type": "string"
        }
      }
    }
  }
}