{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "FarmingCoopAsset.json",
  "title": "FarmingCoopAsset",
  "description": "FarmingCoopAsset asset type",
  "type": "object",
  "allOf": [
    {
      "$ref": "base.schema.json"
    }
  ],
  "properties": {
    "MaxResidents": {
      "type": "integer",
      "hytale": {
        "type": "integer",
        "inheritsProperty": true
      },
      "default": 0
    },
    "ProduceDrops": {
      "type": [
        "object",
        "null"
      ],
      "title": "Map",
      "hytale": {
        "type": "object"
      },
      "additionalProperties": {
        "anyOf": [
          {
            "type": "string",
            "title": "Reference to ItemDropList",
            "hytale": {
              "type": "string"
            }
          },
          {
            "$ref": "ItemDropList.json#"
          }
        ],
        "hytaleAssetRef": "ItemDropList"
      }
    },
    "ResidentSpawnOffset": {
      "default": {
        "X": 0,
        "Y": 0,
        "Z": 0
      },
      "$ref": "common.json#/definitions/Vector3d"
    },
    "ResidentRoamTime": {
      "type": [
        "array",
        "null"
      ],
      "title": "IntRange",
      "hytale": {
        "type": "array"
      },
      "items": [
        {
          "type": "integer",
          "hytale": {
            "type": "integer"
          }
        },
        {
          "type": "integer",
          "hytale": {
            "type": "integer"
          }
        }
      ],
      "minItems": 2,
      "maxItems": 2
    },
    "CaptureWildNPCsInRange": {
      "type": "boolean",
      "hytale": {
        "type": "boolean"
      },
      "default": false
    },
    "WildCaptureRadius": {
      "anyOf": [
        {
          "anyOf": [
            {
              "type": "number",
              "hytale": {
                "type": "number"
              },
              "default": 0
            },
            {
              "type": "string",
              "hytale": {
                "type": "string"
              },
              "pattern": "^(-?Infinity|NaN)$"
            }
          ],
          "hytale": {
            "type": "Number"
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "AcceptedNpcGroups": {
      "type": [
        "array",
        "null"
      ],
      "hytale": {
        "type": "array",
        "inheritsProperty": true
      },
      "items": {
        "anyOf": [
          {
            "type": "string",
            "title": "Reference to NPCGroup",
            "hytale": {
              "type": "string"
            }
          },
          {
            "$ref": "NPCGroup.json#"
          }
        ],
        "hytaleAssetRef": "NPCGroup"
      }
    }
  }
}