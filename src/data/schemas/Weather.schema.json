    "Stars": {
      "type": [
        "string",
        "null"
      ],
      "hytale": {
        "type": "string",
        "inheritsProperty": true,
        "uiCollapsedByDefault": false
      },
      "hytaleCommonAsset": {
        "requiredRoots": [
          "Sky/"
        ],
        "requiredExtension": "png",
        "isUIAsset": false
      }
    },
